package com.mycompany.whilebucle;

/**
 *
 * @author macbook
 */
public class DoWhileBucle {
  public static void main(String[] args){
    int contador = 0;
    do{
      contador++;
      System.out.println(contador + " Ejecuta algo");
    }while(contador < 5);
  }
}
